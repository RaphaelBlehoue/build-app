{
  "name": "build-app",
  "version": "0.1.0",
  "description": "building Entreprise Application",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel src -d dist",
    "serve": "yarn run build && node dist/index.js",
    "watch": "nodemon -w src --exec yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphydev/build-app.git"
  },
  "keywords": [
    "Node.js",
    "express.js",
    "react.js",
    "tdd",
    "bdd"
  ],
  "author": "Raphael SAMPAN <raphaelsampan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raphydev/build-app/issues"
  },
  "homepage": "https://github.com/raphydev/build-app#readme",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/register": "^7.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.1",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3"
  }
}
